{
	"editor.formatOnSave": true,
	"editor.formatOnPaste": true,
	"editor.defaultFormatter": "dprint.dprint",
	"[json]": {
		"editor.defaultFormatter": "dprint.dprint"
	},
	"typescript.tsdk": "node_modules/typescript/lib",
	"typescript.enablePromptUseWorkspaceTsdk": true,
	"files.associations": {
		"*.css": "tailwindcss",
	},
	"files.exclude": {
		".next": true,
		"pages": true,
		"node_modules": true,
		".env.example": true,
		"next-env.d.ts": true,
		"postcss.config.js": true,
		"yarn.lock": true
	}
}